question==>https://codeforces.com/contest/1593/problem/D1

#include<bits/stdc++.h>
#define int long long 
using namespace std;
signed main(){
    int t;
    cin>>t;
    while(t--){
        int n;
        cin>>n;
        int a[n];
        for(int i=0;i<n;i++){
            cin>>a[i];
        }
        int t=0;
        sort(a,a+n);                        // sorting in ascending order because elements can be get small,,, we cannot increase element,,,,,so every element have to same as smallest element of array
                                            // otherwise print -1;      if difference between every element and a[0] is divisible by same number
        for(int i=0;i<n;i++){
            t = __gcd(t,a[i]-a[0]);
        }
        if(t==0){
            cout<<"-1\n";
        }
        else{
            cout<<t<<endl;
        }
    }
    return 0;
}
