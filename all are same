question==>https://codeforces.com/contest/1593/problem/D1

#include<bits/stdc++.h>
#define int long long 
using namespace std;
signed main(){
    int t;
    cin>>t;
    while(t--){
        int n;
        cin>>n;
        int a[n];
        for(int i=0;i<n;i++){
            cin>>a[i];
        }
        int t=0;
        sort(a,a+n);
        for(int i=0;i<n;i++){
            t = __gcd(t,a[i]-a[0]);
        }
        if(t==0){
            cout<<"-1\n";
        }
        else{
            cout<<t<<endl;
        }
    }
    return 0;
}
